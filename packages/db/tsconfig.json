{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": ".",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "index.ts",
    "prisma/**/*.ts",
    "generated/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}